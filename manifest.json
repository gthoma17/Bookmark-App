{
	"name": "Class A",
	"description": "!!!Generic Description!!!",
	"version": "1.3",
	"manifest_version": 2,
	"icons": {
		"128": "128.png"
	},
	"content_scripts":[
		{
			"matches": ["!!!MYWEBADDRESS!!!/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"app": {
		"urls": [
			"!!!MYWEBADDRESS!!!"
		],
		"launch": {
			"container": "panel",
			"height": 768,
			"width": 1366,
			"web_url": "!!!MYWEBADDRESS!!!"
		}
	},
	"permissions": [
		"unlimitedStorage",
		"notifications"
		]
}